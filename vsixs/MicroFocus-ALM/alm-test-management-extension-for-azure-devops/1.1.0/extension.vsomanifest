{
    "manifestVersion": 1,
    "demands": [
        "api-version/3.0"
    ],
    "scopes": [
        "vso.work",
        "vso.project"
    ],
    "contributions": [
        {
            "id": "alm-settings",
            "type": "ms.vss-web.hub",
            "description": "The hub in Organization Settings is used to configure ALM server settings",
            "targets": [
                "ms.vss-web.collection-admin-hub-group"
            ],
            "properties": {
                "name": "ALM Settings",
                "uri": "dist/alm-settings/settings.html"
            }
        },
        {
            "id": "alm-test-coverage",
            "type": "ms.vss-work-web.work-item-form-page",
            "description": "The page in the work item is used to show ALM test coverage information.",
            "targets": [
                "ms.vss-work-web.work-item-form"
            ],
            "properties": {
                "name": "ALM Test Coverage",
                "uri": "dist/test-coverage/coverage-page.html"
            }
        }
    ],
    "contributionTypes": []
}