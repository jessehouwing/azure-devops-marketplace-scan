    <Categories>Plan and track</Categories>
    <Tags>copy,work item</Tags>
    <Properties>
      <Property Id="Microsoft.VisualStudio.Services.Links.GitHub" Value="https://github.com/TravisTX/tfs-wi-copy"/>
    </Properties>
  </Metadata>
  <Dependencies/>
