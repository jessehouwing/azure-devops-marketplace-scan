<?xml version="1.0" encoding="utf-8"?>
<PackageManifest Version="2.0.0" xmlns="http://schemas.microsoft.com/developer/vsx-schema/2011" xmlns:d="http://schemas.microsoft.com/developer/vsx-schema-design/2011">
  <Metadata>
    <Identity Language="en-US" Id="ado-source-cat" Publisher="Magenic" Version="0.2.42"/>
    <DisplayName>Source Code Repository Categorization</DisplayName>
    <Description xml:space="preserve">A tool that adds a page to categorize git source code repositories in a tree structure. This is useful for organizations with a large number of git repositories in a single project.</Description>
    <GalleryFlags>Public</GalleryFlags>
    <Tags>git,organization</Tags>
    <Properties>
      <Property Id="Microsoft.VisualStudio.Services.Links.Getstarted" Value="https://github.com/Magenic/AzureDevOpsGitOrganization"/>
      <Property Id="Microsoft.VisualStudio.Services.Links.License" Value="https://github.com/Magenic/AzureDevOpsGitOrganization"/>
      <Property Id="Microsoft.VisualStudio.Services.Links.Support" Value="https://github.com/Magenic/AzureDevOpsGitOrganization"/>
      <Property Id="Microsoft.VisualStudio.Services.Branding.Color" Value="#78be43"/>
      <Property Id="Microsoft.VisualStudio.Services.Branding.Theme" Value="light"/>
    </Properties>
    <Categories>Azure Repos</Categories>
    <Icon>icon.png</Icon>
  </Metadata>
  <Dependencies/>
  <Installation>
    <InstallationTarget Id="Microsoft.VisualStudio.Services"/>
  </Installation>
  <Assets>
    <Asset Type="Microsoft.VisualStudio.Services.Icons.Default" d:Source="File" Path="icon.png" Addressable="true"/>
    <Asset Type="icon.png" d:Source="File" Path="icon.png" Addressable="true"/>
    <Asset Type="Microsoft.VisualStudio.Services.Content.Details" d:Source="File" Path="documents/overview.md" Addressable="true"/>
    <Asset Type="Microsoft.VisualStudio.Services.Screenshots.1" d:Source="File" Path="images/screenshot.png" Addressable="true"/>
    <Asset Type="directory-categorization.html" d:Source="File" Path="directory-categorization.html" Addressable="true"/>
    <Asset Type="directory-name-form.html" d:Source="File" Path="directory-name-form.html" Addressable="true"/>
    <Asset Type="select-repository.html" d:Source="File" Path="select-repository.html" Addressable="true"/>
    <Asset Type="edit-json.html" d:Source="File" Path="edit-json.html" Addressable="true"/>
    <Asset Type="sdk/scripts/.DS_Store" d:Source="File" Path="sdk/scripts/.DS_Store" Addressable="true"/>
    <Asset Type="sdk/scripts/VSS.SDK.js" d:Source="File" Path="sdk/scripts/VSS.SDK.js" Addressable="true"/>
    <Asset Type="scripts/jquery-3.4.1.min.js" d:Source="File" Path="scripts/jquery-3.4.1.min.js" Addressable="true"/>
    <Asset Type="scripts/repositorytree.js" d:Source="File" Path="scripts/repositorytree.js" Addressable="true"/>
    <Asset Type="Microsoft.VisualStudio.Services.Manifest" d:Source="File" Path="extension.vsomanifest" Addressable="true"/>
  </Assets>
</PackageManifest>