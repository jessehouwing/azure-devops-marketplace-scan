<?xml version="1.0" encoding="utf-8"?>
<PackageManifest Version="2.0.0" xmlns="http://schemas.microsoft.com/developer/vsx-schema/2011" xmlns:d="http://schemas.microsoft.com/developer/vsx-schema-design/2011">
  <Metadata>
    <Identity Language="en-US" Id="ignite-build-extension" Publisher="HenningKrause" Version="1.0.15"/>
    <DisplayName>Ignite</DisplayName>
    <Description xml:space="preserve">Executes setup bundles created with Burn and uploads the build logs.</Description>
    <GalleryFlags>Public</GalleryFlags>
    <Categories>Azure Pipelines</Categories>
    <Tags>release,build,wix,wixtoolkit,burn,bundle,setup,install,bootstrap,bootstrapper</Tags>
    <Properties>
      <Property Id="Microsoft.VisualStudio.Services.Links.Getstarted" Value="https://github.com/henning-krause/ignite"/>
      <Property Id="Microsoft.VisualStudio.Services.Links.Support" Value="https://github.com/henning-krause/ignite/issues"/>
      <Property Id="Microsoft.VisualStudio.Services.Links.GitHub" Value="https://github.com/henning-krause/ignite"/>
    </Properties>
    <Icon>images/fire.png</Icon>
  </Metadata>
  <Dependencies/>
  <Installation>
    <InstallationTarget Id="Microsoft.VisualStudio.Services.Integration"/>
  </Installation>
  <Assets>
    <Asset Type="igniteTask/icon.png" d:Source="File" Path="igniteTask/icon.png" Addressable="true"/>
    <Asset Type="igniteTask/index.js" d:Source="File" Path="igniteTask/index.js" Addressable="true"/>
    <Asset Type="igniteTask/task.json" d:Source="File" Path="igniteTask/task.json" Addressable="true"/>
    <Asset Type="screenshots/configuration.png" d:Source="File" Path="screenshots/configuration.png" Addressable="true"/>
    <Asset Type="Microsoft.VisualStudio.Services.Screenshots.1" d:Source="File" Path="screenshots/configuration.png" Addressable="true"/>
    <Asset Type="Microsoft.VisualStudio.Services.Icons.Default" d:Source="File" Path="images/fire.png" Addressable="true"/>
    <Asset Type="Microsoft.VisualStudio.Services.Content.Details" d:Source="File" Path="details.md" Addressable="true"/>
  </Assets>
</PackageManifest>