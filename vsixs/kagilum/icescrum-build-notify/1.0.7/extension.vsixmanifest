    <GalleryFlags>Public</GalleryFlags>
    <Categories>Integrate</Categories>
    <Tags>agile,scrum,project management,roadmap</Tags>
    <Properties>
      <Property Id="Microsoft.VisualStudio.Services.Links.Getstarted" Value="https://www.icescrum.com/features/"/>
