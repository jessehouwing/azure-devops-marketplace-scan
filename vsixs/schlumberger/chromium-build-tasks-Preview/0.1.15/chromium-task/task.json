{

  "id": "3986AF1E-6143-4578-91AB-62323A6FB618",
  "name": "chromium-task",
  "friendlyName": "Download Chromium Build",
  "description": "Deploys Chromium to the Build Agent",
  "helpMarkDown": "v$(GITVERSION_FullSemVer) [More Information](https://github.com/Schlumberger/vsts-chromium-task#readme)",
  "category": "Deploy",
  "visibility": [
    "Build",
    "Release"
  ],
  "author": "Schlumberger ",
  "version": {
    "Major": "0",
    "Minor": "1",
    "Patch": "15"
  },
  "minimumAgentVersion": "1.92.0",
  "groups": [
    {
      "name": "missing",
      "displayName": "Missing variables",
      "isExpanded": true
    }
  ],
  "inputs": [
    {
      "name": "unpackFolder",
      "type": "filePath",
      "label": "Unpack To",
      "defaultValue": "$(System.DefaultWorkingDirectory)",
      "required": true,
      "helpMarkDown": "Location to unpack the Chromium files to."
    }
  ],
  "instanceNameFormat": "Downloads Chromium to $(unpackFolder)",
  "execution": {
    "PowerShell3": {
      "target": "$(currentDirectory)\\download-chromium-build.ps1",
      "argumentFormat": "",
      "workingDirectory": "$(System.DefaultWorkingDirectory)"
    }
  }
}