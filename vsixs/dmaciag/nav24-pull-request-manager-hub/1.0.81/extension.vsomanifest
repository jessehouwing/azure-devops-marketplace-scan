{
    "manifestVersion": 1,
    "demands": [
        "api-version/5.1"
    ],
    "scopes": [
        "vso.code",
        "vso.build"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/cribeiro84/azure-devops-pull-request-hub"
    },
    "contributions": [
        {
            "id": "showProperties",
            "type": "ms.vss-web.action",
            "description": "Shows the target properties for menu actions for source files",
            "targets": [
                "ms.vss-code-web.source-item-menu"
            ],
            "properties": {
                "name": "Pull Request Manager Hub",
                "title": "ms.vss-code-web.source-item-menu",
                "uri": "build/index.html",
                "group": "actions",
                "icon": {
                    "light": "marketplace/icon-light.png",
                    "dark": "marketplace/icon-dark.png"
                }
            }
        }
    ],
    "contributionTypes": []
}