{
    "manifestVersion": 1,
    "author": "Anthony Simmon",
    "scopes": [
        "vso.build_execute",
        "vso.release_execute"
    ],
    "demands": [
        "api-version/3.0"
    ],
    "contributions": [
        {
            "id": "build-lighthouse-tab",
            "type": "ms.vss-build-web.build-results-tab",
            "description": "Display Google Lighthouse reports and evaluate audit scores with assertions.",
            "targets": [
                "ms.vss-build-web.build-results-view"
            ],
            "properties": {
                "name": "Lighthouse",
                "uri": "tab-build.html",
                "registeredObjectId": "registeredEnvironmentObject",
                "supportsTasks": [
                    "b18196d0-d3ed-11e8-ada4-43a6658503da"
                ],
                "dynamic": true
            },
            "includes": [
                "ms.vss-releaseManagement-web.release-service-data-external"
            ]
        },
        {
            "id": "release-stage-lighthouse-tab",
            "type": "ms.vss-web.tab",
            "description": "Display Google Lighthouse reports and evaluate audit scores with assertions.",
            "targets": [
                "ms.vss-releaseManagement-web.release-environment-editor-tab"
            ],
            "properties": {
                "name": "Lighthouse",
                "uri": "tab-release.html",
                "registeredObjectId": "registeredEnvironmentObject",
                "dynamic": true
            },
            "includes": [
                "ms.vss-releaseManagement-web.release-service-data-external"
            ]
        },
        {
            "id": "Lighthouse",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "Tasks/Lighthouse"
            }
        }
    ],
    "repository": {
        "type": "git",
        "uri": "https://github.com/gsoft-inc/azure-pipelines-lighthouse"
    },
    "contributionTypes": []
}