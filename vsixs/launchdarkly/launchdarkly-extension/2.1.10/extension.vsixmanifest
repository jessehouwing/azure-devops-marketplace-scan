<?xml version="1.0" encoding="utf-8"?>
<PackageManifest Version="2.0.0" xmlns="http://schemas.microsoft.com/developer/vsx-schema/2011" xmlns:d="http://schemas.microsoft.com/developer/vsx-schema-design/2011">
  <Metadata>
    <Identity Language="en-US" Id="launchdarkly-extension" Version="2.1.23" Publisher="launchdarkly"/>
    <DisplayName>LaunchDarkly Integration V2</DisplayName>
    <Description xml:space="preserve">Automatically rollout your LaunchDarkly feature flags during release.</Description>
    <GalleryFlags>Public</GalleryFlags>
    <Categories>Build and release</Categories>
    <Tags>devops,continuous delivery,continuous deployment,feature flag,feature flagging,feature toggle,canary release,dark launch,a/b testing,release</Tags>
    <Properties>
      <Property Id="Microsoft.VisualStudio.Services.Links.Getstarted" Value="http://docs.launchdarkly.com/docs/visual-studio-team-services-extension"/>
      <Property Id="Microsoft.VisualStudio.Services.Links.Learn" Value="http://docs.launchdarkly.com/"/>
      <Property Id="Microsoft.VisualStudio.Services.Links.Support" Value="https://launchdarkly.com/support.html"/>
      <Property Id="Microsoft.VisualStudio.Services.Branding.Color" Value="#ebf4fc"/>
      <Property Id="Microsoft.VisualStudio.Services.Branding.Theme" Value="light"/>
    </Properties>
    <Icon>static/images/launchdarkly.png</Icon>
  </Metadata>
  <Dependencies/>
  <Installation>
    <InstallationTarget Id="Microsoft.VisualStudio.Services"/>
  </Installation>
  <Assets>
    <Asset Type="static/workItemPage.html" d:Source="File" Path="static/workItemPage.html" Addressable="true"/>
    <Asset Type="static/rmSummarySection.html" d:Source="File" Path="static/rmSummarySection.html" Addressable="true"/>
    <Asset Type="Microsoft.VisualStudio.Services.Icons.Default" d:Source="File" Path="static/images/launchdarkly.png" Addressable="true"/>
    <Asset Type="Microsoft.VisualStudio.Services.Screenshots.1" d:Source="File" Path="static/images/work-item-tab.jpg" Addressable="true"/>
    <Asset Type="Microsoft.VisualStudio.Services.Screenshots.2" d:Source="File" Path="static/images/release-task.jpg" Addressable="true"/>
    <Asset Type="Microsoft.VisualStudio.Services.Screenshots.3" d:Source="File" Path="static/images/release-summary.jpg" Addressable="true"/>
    <Asset Type="Microsoft.VisualStudio.Services.Screenshots.4" d:Source="File" Path="static/images/ld-dashboard.jpg" Addressable="true"/>
    <Asset Type="Microsoft.VisualStudio.Services.Content.Details" d:Source="File" Path="overview.md" Addressable="true"/>
    <Asset Type="Microsoft.VisualStudio.Services.Manifest" d:Source="File" Path="extension.vsomanifest" Addressable="true"/>
  </Assets>
</PackageManifest>