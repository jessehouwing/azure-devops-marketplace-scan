{
    "manifestVersion": 1,
    "contributions": [
        {
            "id": "form-group",
            "type": "ms.vss-work-web.work-item-form-group",
            "description": "The panel injected to the work item page",
            "targets": [
                "ms.vss-work-web.work-item-form"
            ],
            "properties": {
                "name": "Blue Canvas",
                "uri": "dist/index.html",
                "height": 200
            }
        },
        {
            "id": "setup-dialog",
            "type": "ms.vss-web.control",
            "description": "The modal window content for settings",
            "targets": [],
            "properties": {
                "uri": "dist/setupDialog.html"
            }
        }
    ],
    "scopes": [
        "vso.work",
        "vso.work_write",
        "vso.work_full"
    ],
    "CustomerQnASupport": {
        "enablemarketplaceqna": false,
        "enableMarketplaceQnA": false
    },
    "contributionTypes": []
}