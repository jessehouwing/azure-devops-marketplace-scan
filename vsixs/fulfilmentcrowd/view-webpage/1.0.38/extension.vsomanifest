{
    "manifestVersion": 1,
    "repository": {
        "type": "git",
        "url": "https://github.com/fulfilmentcrowd/azure-boards-view-webpage"
    },
    "contributions": [
        {
            "id": "view-webpage",
            "type": "ms.vss-work-web.work-item-form-control",
            "targets": [
                "ms.vss-work-web.work-item-form"
            ],
            "properties": {
                "name": "View Webpage",
                "uri": "view-webpage.html",
                "height": "200",
                "inputs": [
                    {
                        "id": "Url",
                        "name": "URL",
                        "description": "URL to open. Use {field1} etc for dynamic data within the url.",
                        "validation": {
                            "dataType": "String",
                            "isRequired": true
                        }
                    },
                    {
                        "id": "ControlHeight",
                        "name": "ControlHeight",
                        "description": "Height of control. If not set, defaults to 200.",
                        "validation": {
                            "dataType": "Number",
                            "isRequired": false
                        }
                    },
                    {
                        "id": "Field1",
                        "name": "Field1",
                        "description": "Select a field for a variable to be used in URL building. Use {field1} in the url to insert this value.",
                        "type": "WorkitemField",
                        "properties": {
                            "workItemFieldTypes": [
                                "String",
                                "Integer",
                                "Double"
                            ]
                        },
                        "validation": {
                            "dataType": "String",
                            "isRequired": false
                        }
                    },
                    {
                        "id": "Field2",
                        "name": "Field2",
                        "description": "Select a field for a variable to be used in URL building. Use {field2} in the url to insert this value.",
                        "type": "WorkitemField",
                        "properties": {
                            "workItemFieldTypes": [
                                "String",
                                "Integer",
                                "Double"
                            ]
                        },
                        "validation": {
                            "dataType": "String",
                            "isRequired": false
                        }
                    },
                    {
                        "id": "Field3",
                        "name": "Field3",
                        "description": "Select a field for a variable to be used in URL building. Use {field3} in the url to insert this value.",
                        "type": "WorkitemField",
                        "properties": {
                            "workItemFieldTypes": [
                                "String",
                                "Integer",
                                "Double"
                            ]
                        },
                        "validation": {
                            "dataType": "String",
                            "isRequired": false
                        }
                    }
                ]
            }
        }
    ],
    "scopes": [],
    "contributionTypes": []
}