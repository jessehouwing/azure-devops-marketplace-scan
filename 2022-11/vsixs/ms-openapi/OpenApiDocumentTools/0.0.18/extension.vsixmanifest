<?xml version="1.0" encoding="utf-8"?>
<PackageManifest Version="2.0.0" xmlns="http://schemas.microsoft.com/developer/vsx-schema/2011" xmlns:d="http://schemas.microsoft.com/developer/vsx-schema-design/2011">
  <Metadata>
    <Identity Language="en-US" Id="OpenApiDocumentTools" Publisher="ms-openapi" Version="0.0.18"/>
    <DisplayName>OpenAPI Document Tools</DisplayName>
    <Properties>
      <Property Id="Microsoft.VisualStudio.Services.Branding.Color" Value="#dcebfc"/>
      <Property Id="Microsoft.VisualStudio.Services.Branding.Theme" Value="light"/>
    </Properties>
    <Categories>Build and release</Categories>
    <Description xml:space="preserve">Suite of tools useful for: (a) auto-generating OpenAPI documents at build time based on your source code + annotations, and (b) publishing OpenAPI documents to Git source control.</Description>
    <GalleryFlags>Preview Public</GalleryFlags>
    <Tags>OpenAPI,Swagger,REST,Swashbuckle</Tags>
    <Icon>img/logo.png</Icon>
    <License>LICENSE</License>
  </Metadata>
  <Dependencies/>
  <Installation>
    <InstallationTarget Id="Microsoft.VisualStudio.Services"/>
  </Installation>
  <Assets>
    <Asset Type="img/logo.png" d:Source="File" Path="img/logo.png" Addressable="true"/>
    <Asset Type="Microsoft.VisualStudio.Services.Icons.Default" d:Source="File" Path="img/logo.png" Addressable="true"/>
    <Asset Type="img/sample.ui.buildOpenApiDocumentsTask.input.png" d:Source="File" Path="img/sample.ui.buildOpenApiDocumentsTask.input.png" Addressable="true"/>
    <Asset Type="img/sample.ui.publishOpenApiDocumentsTask.input.png" d:Source="File" Path="img/sample.ui.publishOpenApiDocumentsTask.input.png" Addressable="true"/>
    <Asset Type="Microsoft.VisualStudio.Services.Content.Details" d:Source="File" Path="README.md" Addressable="true"/>
    <Asset Type="Microsoft.VisualStudio.Services.Content.License" d:Source="File" Path="LICENSE" Addressable="true"/>
    <Asset Type="Microsoft.VisualStudio.Services.Manifest" d:Source="File" Path="extension.vsomanifest" Addressable="true"/>
  </Assets>
</PackageManifest>