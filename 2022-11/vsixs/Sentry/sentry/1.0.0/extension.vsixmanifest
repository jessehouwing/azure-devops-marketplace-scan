<?xml version="1.0" encoding="utf-8"?>
<PackageManifest Version="2.0.0" xmlns="http://schemas.microsoft.com/developer/vsx-schema/2011" xmlns:d="http://schemas.microsoft.com/developer/vsx-schema-design/2011">
  <Metadata>
    <Identity Language="en-US" Id="sentry" Publisher="Sentry" Version="1.0.0"/>
    <DisplayName>Sentry</DisplayName>
    <Description xml:space="preserve">Open-source error tracking that helps developers monitor and fix crashes in real time. Iterate continuously. Boost efficiency. Improve user experience.</Description>
    <GalleryFlags>Public</GalleryFlags>
    <Categories>Code,Build and release,Collaborate</Categories>
    <Properties>
      <Property Id="Microsoft.VisualStudio.Services.GalleryProperties.PostAcquisitionConfigLink" Value="https://sentry.io/extensions/vsts/configure/?targetId={{targetId}}&amp;targetName={{targetName}}&amp;targetUri={{targetUri}}"/>
      <Property Id="Microsoft.VisualStudio.Services.GalleryProperties.PostAcquisitionConfigLabel" Value="Configure Sentry"/>
    </Properties>
    <Icon>images/logo.png</Icon>
  </Metadata>
  <Dependencies/>
  <Installation>
    <InstallationTarget Id="Microsoft.VisualStudio.Services.Cloud"/>
  </Installation>
  <Assets>
    <Asset Type="images/exception.png" d:Source="File" Path="images/exception.png" Addressable="true"/>
    <Asset Type="images/integration-config.png" d:Source="File" Path="images/integration-config.png" Addressable="true"/>
    <Asset Type="images/suspect-commit.png" d:Source="File" Path="images/suspect-commit.png" Addressable="true"/>
    <Asset Type="images/get-it-free.png" d:Source="File" Path="images/get-it-free.png" Addressable="true"/>
    <Asset Type="images/select-vsts-account.png" d:Source="File" Path="images/select-vsts-account.png" Addressable="true"/>
    <Asset Type="images/configure-sentry.png" d:Source="File" Path="images/configure-sentry.png" Addressable="true"/>
    <Asset Type="images/sentry-login.png" d:Source="File" Path="images/sentry-login.png" Addressable="true"/>
    <Asset Type="images/select-org.png" d:Source="File" Path="images/select-org.png" Addressable="true"/>
    <Asset Type="Microsoft.VisualStudio.Services.Icons.Default" d:Source="File" Path="images/logo.png" Addressable="true"/>
    <Asset Type="Microsoft.VisualStudio.Services.Content.Details" d:Source="File" Path="overview.md" Addressable="true"/>
    <Asset Type="Microsoft.VisualStudio.Services.Manifest" d:Source="File" Path="extension.vsomanifest" Addressable="true"/>
  </Assets>
</PackageManifest>