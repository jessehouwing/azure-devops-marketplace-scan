<?xml version="1.0" encoding="utf-8"?>
<PackageManifest Version="2.0.0" xmlns="http://schemas.microsoft.com/developer/vsx-schema/2011" xmlns:d="http://schemas.microsoft.com/developer/vsx-schema-design/2011">
  <Metadata>
    <Identity Language="en-US" Id="net-core-version-extractor" Version="1.0.1" Publisher="MatthewdeBeer"/>
    <DisplayName>.NET Core / Standard Version Extractor</DisplayName>
    <Tags>Application Version,Version,csproj,.NET Core,.NET Standard</Tags>
    <GalleryFlags>Public</GalleryFlags>
    <Properties>
      <Property Id="Microsoft.VisualStudio.Services.Links.Support" Value="https://github.com/setagana/NetCoreVersionExtractor/issues"/>
      <Property Id="Microsoft.VisualStudio.Services.Links.Repository" Value="https://github.com/setagana/NetCoreVersionExtractor"/>
      <Property Id="Microsoft.VisualStudio.Services.Links.GitHub" Value="https://github.com/setagana/NetCoreVersionExtractor"/>
    </Properties>
    <Description xml:space="preserve">Useful for retrieving the &lt;Version&gt; element from a .NET Core / Standard csproj file.</Description>
    <Categories>Azure Pipelines</Categories>
    <License>LICENSE</License>
    <Icon>images/extension-icon.png</Icon>
  </Metadata>
  <Dependencies/>
  <Installation>
    <InstallationTarget Id="Microsoft.VisualStudio.Services"/>
  </Installation>
  <Assets>
    <Asset Type="Microsoft.VisualStudio.Services.Content.License" d:Source="File" Path="LICENSE" Addressable="true"/>
    <Asset Type="Microsoft.VisualStudio.Services.Content.Details" d:Source="File" Path="README.md" Addressable="true"/>
    <Asset Type="Microsoft.VisualStudio.Services.Screenshots.1" d:Source="File" Path="images/VersionExtractor.png" Addressable="true"/>
    <Asset Type="Microsoft.VisualStudio.Services.Icons.Default" d:Source="File" Path="images/extension-icon.png" Addressable="true"/>
    <Asset Type="Microsoft.VisualStudio.Services.Manifest" d:Source="File" Path="extension.vsomanifest" Addressable="true"/>
  </Assets>
