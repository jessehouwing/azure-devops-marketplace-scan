<?xml version="1.0" encoding="utf-8"?>
<PackageManifest Version="2.0.0" xmlns="http://schemas.microsoft.com/developer/vsx-schema/2011" xmlns:d="http://schemas.microsoft.com/developer/vsx-schema-design/2011">
  <Metadata>
    <Identity Language="en-US" Id="rapid7-insightappsec-extension" Version="1.2.3" Publisher="rapid7"/>
    <DisplayName>Rapid7 InsightAppSec</DisplayName>
    <GalleryFlags>Public</GalleryFlags>
    <Description xml:space="preserve">Integrate Rapid7 InsightAppSec into your Azure DevOps build and release pipelines to automatically crawl and assess web applications to identify vulnerabilities.</Description>
    <Categories>Azure Pipelines</Categories>
    <Tags>DAST,SDLC,AppSec,Security,build,release</Tags>
    <Properties>
      <Property Id="Microsoft.VisualStudio.Services.Branding.Color" Value="#ececec"/>
      <Property Id="Microsoft.VisualStudio.Services.Branding.Theme" Value="light"/>
      <Property Id="Microsoft.VisualStudio.Services.Links.Getstarted" Value="https://insightappsec.help.rapid7.com/docs/azure-devops-integration"/>
      <Property Id="Microsoft.VisualStudio.Services.Links.License" Value="https://github.com/rapid7/insightappsec-azure-devops-extension/blob/master/LICENSE.txt"/>
      <Property Id="Microsoft.VisualStudio.Services.Links.Support" Value="https://www.rapid7.com/for-customers/"/>
      <Property Id="Microsoft.VisualStudio.Services.Links.Issues" Value="https://github.com/rapid7/insightappsec-azure-devops-extension/issues"/>
      <Property Id="Microsoft.VisualStudio.Services.Links.GitHub" Value="https://github.com/rapid7/insightappsec-azure-devops-extension"/>
    </Properties>
    <License>LICENSE.txt</License>
    <Icon>images/rapid7.png</Icon>
  </Metadata>
  <Dependencies/>
  <Installation>
    <InstallationTarget Id="Microsoft.VisualStudio.Services"/>
  </Installation>
  <Assets>
    <Asset Type="Microsoft.VisualStudio.Services.Content.Details" d:Source="File" Path="overview.md" Addressable="true"/>
    <Asset Type="Microsoft.VisualStudio.Services.Content.License" d:Source="File" Path="LICENSE.txt" Addressable="true"/>
    <Asset Type="Microsoft.VisualStudio.Services.Icons.Default" d:Source="File" Path="images/rapid7.png" Addressable="true"/>
    <Asset Type="Microsoft.VisualStudio.Services.Manifest" d:Source="File" Path="extension.vsomanifest" Addressable="true"/>
  </Assets>
</PackageManifest>