    {
      "name": "taskProfile",
      "type": "pickList",
      "label": "Task profile",
      "required": true,
      "groupName": "connectionConfig",
      "options": {
        "Console": "Console",
        "Local": "Local"
      },
      "helpMarkDown": "Select task profile: local or console"
    },
    {
      "name": "HdivConsoleService",
      "type": "connectedService:HdivConsole",
      "label": "Hdiv console connection",
      "defaultValue": "",
      "required": true,
      "groupName": "connectionConfig",
      "helpMarkDown": "Select the Hdiv console connection to use.",
      "visibleRule": "taskProfile == Console"
    },
    {
      "name": "logPath",
      "type": "string",
      "label": "Hdiv log path",
      "defaultValue": "/home/vsts/work/1/s/hdivAgentLog.hlg",
      "required": true,
      "groupName": "connectionConfig",
      "helpMarkDown": "Set log file location path. Task will retrieve vulnerability information from a local log file created by Hdiv Agent (hdivAgentLog.hlg).",
      "visibleRule": "taskProfile == Local"
    },
    {
      "name": "application",
      "type": "string",
      "label": "Application",
      "defaultValue": "",
      "required": true,
      "groupName": "appConfig",
      "helpMarkDown": "Set the application name to process. "
    },
    {
      "name": "applicationVersion",
      "type": "string",
      "label": "Application version",
      "required": false,
      "groupName": "appConfig",
      "helpMarkDown": "Set application version. You can use pipeline provided variables like $(Build.BuildNumber). If you leave it empty, all vulnerabilities in the application will be considered, regardless of the version to which they belong."
    },
    {
      "name": "blStartDate",
      "label": "The start date matches the beginning of this build",
       "type": "boolean",
      "defaultValue": "true",
      "required": false,
       "groupName": "dateConfig"
    },
    {
      "name": "startDate",
      "label": "Initial date value ",
      "type": "string",
      "groupName": "dateConfig",
      "required": false,
      "helpMarkDown": "Set end date with format yyyy/MM/dd HH:mm:ss. You can use predefined pipeline variables.",
      "visibleRule": "blStartDate == false" 
    },
    {
      "name": "blEndDate",
      "label": "End date match with build end ",
      "type": "boolean",
      "defaultValue": "true",
      "required": false,
      "groupName": "dateConfig"
    },
    {
      "name": "endDate",
      "label": "End date  value ",
      "type": "string",
      "required": false,
      "groupName": "dateConfig",
      "helpMarkDown": "Set end date with format yyyy/MM/dd HH:mm:ss",
